[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = HelloWorld
[access "refs/*"]
	owner = group user/skrish-8
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/skrish-8
	label-Verified = -1..+1 group user/skrish-8
	push = group user/skrish-8
	read = group user/skrish-8
	rebase = group user/skrish-8
	removeReviewer = group user/skrish-8
	submit = group user/skrish-8
[access "refs/heads/*"]
	create = group user/skrish-8
	push = +force group user/skrish-8
	pushMerge = group user/skrish-8
	read = group user/skrish-8
[access "refs/tags/*"]
	push = group user/skrish-8
	push = +force group user/skrish-8
